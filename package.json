{
  "name": "@jupiterone/jupiterone-client-nodejs",
  "version": "0.21.1",
  "description": "A node.js client wrapper for JupiterOne public API",
  "main": "src/j1client.js",
  "repository": "git@github.com:JupiterOne/jupiterone-client-nodejs.git",
  "author": "JupiterOne <dev@jupiterone.io>",
  "license": "MIT",
  "files": [
    "src",
    "bin"
  ],
  "bin": {
    "j1": "bin/j1"
  },
  "scripts": {
    "env": "export $(grep -v '^#' .env | xargs);",
    "format": "yarn prettier --write '**/*.{ts,js,json,md}'",
    "test": "jest",
    "test:upsert": "yarn env; node src/j1cli.js -o upsert --entity --account $J1_ACCOUNT_ID --key $J1_API_TOKEN --input-file ./examples-yaml/entities.yml"
  },
  "dependencies": {
    "@jupiterone/jupiterone-alert-rules": "^0.3.0",
    "@pollyjs/adapter-node-http": "^2.7.0",
    "@pollyjs/core": "^2.6.3",
    "amazon-cognito-identity-js-node": "^0.0.3",
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-link": "^1.2.11",
    "apollo-link-batch-http": "^1.2.11",
    "apollo-link-http": "^1.5.12",
    "apollo-link-retry": "^2.2.13",
    "commander": "^2.14.1",
    "file-set": "^2.0.0",
    "fs-extra": "^5.0.0",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "inquirer": "^5.1.0",
    "js-yaml": "^3.13.1",
    "moment": "^2.22.2",
    "mustache": "^2.3.0",
    "node-fetch": "^2.3.0",
    "p-all": "^2.1.0",
    "p-map": "^1.2.0",
    "papaparse": "^4.6.3",
    "pluralize": "^7.0.0",
    "progress": "^2.0.3",
    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "strip-ansi": "^4.0.0",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "dotenv": "^7.0.0",
    "husky": "^2.4.0",
    "jest": "^24.9.0",
    "lint-staged": "^8.2.0",
    "prettier": "^1.18.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{ts,tsx,js,jsx,json,css,md}": [
        "yarn format",
        "git add"
      ]
    },
    "ignore": []
  }
}
