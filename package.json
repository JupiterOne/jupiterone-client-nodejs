{
  "name": "@jupiterone/jupiterone-client-nodejs",
  "version": "0.22.1",
  "description": "A node.js client wrapper for JupiterOne public API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "git@github.com:JupiterOne/jupiterone-client-nodejs.git",
  "author": "JupiterOne <dev@jupiterone.io>",
  "license": "MIT",
  "files": [
    "dist",
    "bin"
  ],
  "bin": {
    "j1": "bin/j1"
  },
  "scripts": {
    "env": "export $(grep -v '^#' .env | xargs);",
    "format": "yarn prettier --write '**/*.{ts,js,json,css,md,yml}'",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --cache --fix --ext .ts,.tsx",
    "pretest": "yarn lint && yarn type-check",
    "test": "jest",
    "test:upsert": "yarn env; node src/j1cli.js -o upsert --entity --account $J1_ACCOUNT_ID --key $J1_API_TOKEN --input-file ./examples-yaml/entities.yml",
    "prepare": "tsc -p tsconfig.dist.json --declaration"
  },
  "dependencies": {
    "@jupiterone/jupiterone-alert-rules": "^0.3.0",
    "@lifeomic/attempt": "^3.0.0",
    "@pollyjs/adapter-node-http": "^2.7.0",
    "@pollyjs/core": "^2.6.3",
    "amazon-cognito-identity-js-node": "^0.0.3",
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-link": "^1.2.11",
    "apollo-link-batch-http": "^1.2.13",
    "apollo-link-retry": "^2.2.13",
    "commander": "^5.0.0",
    "eslint-config-prettier": "^6.10.1",
    "file-set": "^2.0.0",
    "fs-extra": "^5.0.0",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.1",
    "inquirer": "^5.1.0",
    "js-yaml": "^3.13.1",
    "moment": "^2.22.2",
    "mustache": "^2.3.0",
    "node-fetch": "^2.6.0",
    "p-all": "^2.1.0",
    "p-map": "^1.2.0",
    "pluralize": "^7.0.0",
    "progress": "^2.0.3",
    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "strip-ansi": "^4.0.0",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.8",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "dotenv": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.8.2",
    "husky": "^2.4.0",
    "jest": "^25.2.4",
    "lint-staged": "^8.2.0",
    "prettier": "^2.0.2",
    "ts-jest": "^25.3.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/"
    ],
    "globals": {
      "ts-jest": {
        "isolatedModules": true
      }
    }
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:jest/recommended",
      "prettier",
      "prettier/@typescript-eslint"
    ],
    "env": {
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "rules": {
      "@typescript-eslint/no-use-before-define": [
        "error",
        {
          "functions": false
        }
      ],
      "@typescript-eslint/no-var-requires": [
        "warn"
      ]
    }
  },
  "prettier": {
    "trailingComma": "all",
    "proseWrap": "always",
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{ts,js,json,css,md,yml}": [
        "yarn format",
        "git add"
      ]
    },
    "ignore": []
  }
}
